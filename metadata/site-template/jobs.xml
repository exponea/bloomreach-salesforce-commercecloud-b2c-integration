    <job job-id="Bloomreach - CustomerFeed (Delta Export)" priority="0">
        <description/>
        <parameters/>
        <split sequential="false">
            <contexts template-ref="template_124">
                <context site-id="RefArch"/>
                <context site-id="SiteGenesis"/>
            </contexts>
            <flow-template template-id="template_124">
                <step step-id="genrateCustomerFeedDelta" type="custom.Exponea.Generate.CustomerInfoFeedDelta" enforce-restart="false">
                    <description/>
                    <parameters>
                        <parameter name="TargetFolder">src/bloomreach/customerInfoFeed</parameter>
                        <parameter name="FileNamePrefix">cusotmer-info-feed-delta</parameter>
                        <parameter name="Query">lastModified &gt; {0}</parameter>
                        <parameter name="MaxNumberOfRows">1000000</parameter>
